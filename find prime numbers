def prime_checker():
    import math
    if n==1:
        print("It's not a prime number.")
    elif n==2:
        print("It's a prime number.")
    else:
        for x in range(2,n):
            if n%x==0:
                print("It's not a prime number.")
        print("It's a prime number.")

