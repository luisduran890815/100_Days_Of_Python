x=list(range(1,101))
lista1=[]
for y in x:
    if y%3==0 and y%5==0:
        y=str("FizzBuzz")
        lista1.append(y)
        #print(x)
    elif y%5==0:
        y=str("Buzz")
        lista1.append(y)
        #print(x)
    elif y%3==0:
        y=str("Fizz")
        lista1.append(y)
        #print(x)
    else:
        lista1.append(y)
print(lista1)
